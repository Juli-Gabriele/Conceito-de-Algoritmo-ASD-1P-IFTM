programa {
  funcao inicio() {
    // Elabore um algoritmo que leia um determinado número
   // e apresente na tela a tabuada de multiplicação deste número.
   inteiro num
   escreva("Digite um número ")
   leia(num)
   para (inteiro c = 0; c < 11; c++){
   escreva( c, " x ",  num , " = ", c * num,  "\n")
   }


  }
}
