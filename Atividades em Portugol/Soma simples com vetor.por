programa {
  //Escreva um algoritmo que leia dez valores numéricos inteiros e apresente na tela o somatório dos valores.
  funcao inicio() {
   inteiro soma = 0
   inteiro num[10] 
   para (inteiro c = 0; c < 10; c++){
   leia (num [c])
   soma = soma + num[c]
   }
   escreva("A soma dos números é ",soma)
  }
}
