programa {
  funcao inicio() {
    real a, b, w, x , y ,z
    escreva("Digite o primeiro número :")
    leia(a)
    escreva("Digite o segundo número :")
    leia(b)
    w = a + b
    x = a - b 
    y = a * b 
    z = a / b 
    escreva (" W = ", w ,"\n")
    escreva (" X = ", x,"\n")
    escreva (" Y = ", y,"\n")
    escreva (" Z = ", z,"\n")

  }
}
