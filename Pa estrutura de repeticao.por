programa {
  funcao inicio() {
   //calcular o ultimo termo de uma  PA, dada a razão 
  // e o primeiro termo
  real razao,a1,an,razao
  inteiro qtd
  escreva("digite o primeiro termo:")
  leia(a1)
  escreva("digite a razao :")
  leia(razao)
  escreva("digite a quantidade de termo :")
  leia(qtd)
  an = a1 + (qtd -1) * razao
  escreva("O primeiro termo é : " ,a1, "O ultimo termo é ",an)

  }
}
