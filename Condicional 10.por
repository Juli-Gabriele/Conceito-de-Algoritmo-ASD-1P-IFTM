programa {
// Faça um programa capaz de ler dois números b e h,
// que representam os lados de um retângulo ou quadrado, e imprimir se ele é um quadrado ou um retângulo.
  funcao inicio() {
    real b,h
   escreva("Digite o valor da base :")
   leia(b)
   escreva("Digite o valor da altura :")
   leia(h)
   se (b == h){
    escreva ("Os dados informados representam os lados de um : Quadrado")

   }senao{
    escreva ("Os dados informados representam os lados de um : Retangulo")
   }


  }
}
